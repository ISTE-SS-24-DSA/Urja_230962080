#include <iostream>
using namespace std;
int main() {
    int a,i,j,k,lar;
    cout<<"Enter the number of elements in array"<<endl;
    cin>>a;
    int N[a];
    cout<<"Enter the elements:"<<endl;
    for(i=0;i<a;i++)
    {
        cin>>N[i];
    }
    lar=N[0];
    for(j=0;j<a;j++)
    {
       if(N[j]>lar)
       lar=N[j];
    }
    cout<<"largest number is:"<<lar<<endl;
    return 0;
}
