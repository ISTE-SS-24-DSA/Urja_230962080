#include <iostream>
using namespace std;
int main() {
    int a,i,count=0;
    cout<<"Enter a number"<<endl;
    cin>>a;
    for(i=2;i<a;i++)
    {
        if(a%i==0)
        {
            count++;
            break;
        }
    }
    if(a>1 && count==0)
        cout<<"Prime number"<<endl;
    else if(a==1)
        cout<<"Not prime or composite"<<endl;
    else
        cout<<"Composite number"<<endl;
}
